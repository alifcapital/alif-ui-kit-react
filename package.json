    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-libcss": "^1.0.6",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.0",
