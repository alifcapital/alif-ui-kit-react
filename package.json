    "type": "git",
    "url": "git+https://github.com/alifcapital/alif-ui-kit-react.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx,css,scss}\"",
    "test": "jest --silent --config jestconfig.json"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "keywords": [
    "ui",
    "react",
    "typescript",
    "design system"
