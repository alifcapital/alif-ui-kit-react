  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "homepage": "https://github.com/alifcapital/alif-ui-kit-react",
  "author": "Alif Bank",
  "license": "ISC",
